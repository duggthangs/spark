    {
      "id": "function-impl",
      "type": "code-selector",
      "title": "Choose Implementation",
      "description": "Select your preferred approach and customize if needed",
      "language": "typescript",
      "options": [
        {
          "id": "recursive",
          "label": "Recursive",
          "code": "function fibonacci(n: number): number {\n  if (n <= 1) return n;\n  return fibonacci(n - 1) + fibonacci(n - 2);\n}\n\n// Usage\nconsole.log(fibonacci(10)); // 55"
        },
        {
          "id": "iterative",
          "label": "Iterative",
          "code": "function fibonacci(n: number): number {\n  let [a, b] = [0, 1];\n  for (let i = 0; i < n; i++) {\n    [a, b] = [b, a + b];\n  }\n  return a;\n}\n\n// Usage\nconsole.log(fibonacci(10)); // 55"
        },
        {
          "id": "memoized",
          "label": "Memoized",
          "code": "const memo: Record<number, number> = {};\n\nfunction fibonacci(n: number): number {\n  if (n in memo) return memo[n];\n  if (n <= 1) return n;\n  return memo[n] = fibonacci(n - 1) + fibonacci(n - 2);\n}\n\n// Usage\nconsole.log(fibonacci(10)); // 55"
        }
      ]
    },
    {
      "id": "budget",
      "type": "numeric-inputs",
      "title": "Budget Allocation",
      "items": [
